{
  "editor.lineHeight" : 22,
  "prettier.singleQuote" : true,
  "editor.formatOnSaveMode" : "modifications",
  "editor.fontFamily" : "Victor mono",
  "window.titleBarStyle" : "native",
  "editor.fontSize" : 14,
  "editor.fontWeight" : "700",
  "workbench.editor.tabSizing" : "shrink",
  "editor.minimap.enabled": false,
  "workbench.iconTheme": "vscode-icons",
  "workbench.sideBar.location" : "right",
  "editor.fontLigatures": true,
  "workbench.editor.showTabs" : false,
  "editor.letterSpacing" : 1.2,
  "bracket-pair-colorizer-2.depreciation-notice": false,
  "eslint.lintTask.enable": true,
  "editor.accessibilitySupport": "off",
  ​"editor.bracketPairColorization.enabled"​: ​true​, 
  "editor.guides.bracketPairs": "active",
  "prettier.embeddedLanguageFormatting": "off",
  "extension.prophet.htmlhint.enabled": false,
  "workbench.colorTheme": "Jetbrains Fleet",
  "errorLens.enabled": true,
  "extension.prophet.script.server.trace.server": "messages",
  "extension.prophet.upload.enabled": true,
  "editor.glyphMargin": true,
  "editor.lineDecorationsWidth": 0,
  "breadcrumbs.enabled": false,
  "extension.prophet.script.server.disable.diagnostics": true,
  "window.commandCenter": true,
  "[javascript]" : {
    "editor.defaultFormatter" : "esbenp.prettier-vscode"
  },
  "stylelint.configOverrides" : {
    "js.validate" : false
  },
  "emmet.includeLanguages" : {
    "isml" : "html"
  },
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "stylelint.snippet" : [
    "css",
    "less",
    "postcss",
    "scss"
  ],
  "errorLens.enabledDiagnosticLevels": [
    "warning",
    "info"
  ],
  "workbench.colorCustomizations": {},
  "workbench.statusBar.visible": false,
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "editor.stickyScroll.enabled": true,
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  }
}