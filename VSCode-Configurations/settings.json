{
  "editor.lineHeight" : 22,
  "prettier.singleQuote" : true,
  "editor.formatOnSaveMode" : "modifications",
  "editor.fontFamily": "Victor mono",
  "editor.fontSize" : 14,
  "editor.fontWeight" : "700",
  "workbench.editor.tabSizing": "shrink",
  "workbench.iconTheme": "Monokai Pro (Filter Spectrum) Icons",
  "editor.fontLigatures": true,
  "editor.letterSpacing" : 1.2,
  "bracket-pair-colorizer-2.depreciation-notice": false,
  "eslint.lintTask.enable": true,
  "editor.accessibilitySupport": "off",
  ​"editor.bracketPairColorization.enabled"​: ​true​, 
  "editor.guides.bracketPairs": "active",
  "prettier.embeddedLanguageFormatting": "off",
  "extension.prophet.htmlhint.enabled": false,
  "errorLens.enabled": true,
  "extension.prophet.script.server.trace.server": "messages",
  "extension.prophet.upload.enabled": true,
  "editor.glyphMargin": true,
  "editor.lineDecorationsWidth": 0,
  "extension.prophet.script.server.disable.diagnostics": true,
  "window.commandCenter": true,
  "stylelint.configOverrides" : {
    "js.validate" : false
  },
  "emmet.includeLanguages" : {
    "isml" : "html"
  },
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "stylelint.snippet" : [
    "css",
    "less",
    "postcss",
    "scss"
  ],
  "errorLens.enabledDiagnosticLevels": [
    "warning",
    "info",
    "error"
  ],
  "workbench.colorCustomizations": {},
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "editor.stickyScroll.enabled": true,
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "git.mergeEditor": true,
  
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "comment"
        ],
        "settings": {
          "fontStyle": "italic"
          // "foreground": "#FF80AB"
        }
      },
      {
        "scope": [
          "variable.parameter"
        ],
        "settings": {
          "fontStyle": "italic",
          "foreground": "#FF8A65"
        }
      }
    ]
  },
  "inlineFold.supportedLanguages": [
    "html",
    "svelte",
    "vue",
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact",
    "isml"
  ],
  "inlineFold.unfoldOnLineSelect": true,
  "editor.minimap.enabled": false,
  "yaml.schemas": {
    "file:///Users/cristianestarlich/.vscode/extensions/atlassian.atlascode-3.0.0/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
  },
  "redhat.telemetry.enabled": false,
  "workbench.colorTheme": "Amethyst Dark",
  "editor.cursorBlinking": "expand",
  "editor.linkedEditing": true,
  "editor.cursorSmoothCaretAnimation": "on",
  "[javascript]": {
    "editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
  },
  "explorer.confirmDragAndDrop": false,
  "workbench.fontAliasing": "antialiased",
  "workbench.editor.highlightModifiedTabs": true,
  "window.nativeTabs": true,
  "search.defaultViewMode": "tree",
  "window.zoomLevel": 1
}
