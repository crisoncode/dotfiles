{
  "name": "tennis-point-mfra",
  "version": "0.1.0",
  "description": "Tennis Point Storefront",
  "main": "",
  "engines": {
    "node": ">=8.0"
  },
  "scripts": {
    "test": "sgmf-scripts --test test/unit/**/*.js",
    "test:appium": "node bin/Makefile functional -- appium -- ",
    "cover": "sgmf-scripts --cover",
    "test:integration": "node bin/Makefile integration -- ",
    "test:functional": "node bin/Makefile functional -- ",
    "test:functional:docker": "bin/test-functional-docker.sh",
    "test:functional:sauce": "./node_modules/.bin/wdio test/functional/webdriver/wdio.conf.js ",
    "compile:scss": "sgmf-scripts --compile css",
    "compile:js": "sgmf-scripts --compile js",
    "zipData": "cd ../data_tp/; npm run zipData",
    "auth": "./node_modules/.bin/sfcc-ci client:auth",
    "data:upload": "../data_tp/node_modules/.bin/sfcc-ci instance:upload ../data_tp/data_tp.zip -D",
    "data:import": "../data_tp/node_modules/.bin/sfcc-ci instance:import data_tp.zip -s",
    "data:update": "npm run zipData; npm run auth; npm run data:upload; npm run data:import;",
    "lint": "npm run lint:css && npm run lint:js",
    "lint:css": "sgmf-scripts --lint css",
    "lint:js": "sgmf-scripts --lint js",
    "upload": "sgmf-scripts --upload",
    "uploadCartridge": "sgmf-scripts --uploadCartridge app_storefront_base && sgmf-scripts --uploadCartridge modules && sgmf-scripts --uploadCartridge bm_app_storefront_base",
    "watch": "sgmf-scripts --watch",
    "release": "node bin/Makefile release --",
    "precommit": "npm run -s lint",
    "copy:images": "cp -r node_modules/flag-icon-css/flags cartridges/app_storefront_common/cartridge/static/default",
    "webpack:prd": "./node_modules/.bin/webpack --env prd",
    "webpack:dev": "webpack --env dev --watch --verbose --progress --debug"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/arvatohightech/tennis-point-mfra.git"
  },
  "homepage": "https://bitbucket.org/arvatohightech/tennis-point-mfra.git#readme",
  "devDependencies": {
    "@opuu/inview": "^1.0.0",
    "@tridnguyen/config": "^2.3.1",
    "babel-eslint": "^10.0.1",
    "babel-plugin-module-resolver": "^2.5.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-remove-strict-mode": "0.0.2",
    "chai": "^3.5.0",
    "chai-subset": "^1.5.0",
    "chalk": "^1.1.3",
    "cheerio": "0.22.0",
    "cldr-data": "^31.0.2",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.28.5",
    "dwupload": "^3.8.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^5.0.1",
    "eslint-import-resolver-babel-module": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-sitegenesis": "~1.0.0",
    "eslint-scope": "^5.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^0.11.2",
    "fs-extra": "^5.0.0",
    "glob": "^7.1.2",
    "globalize": "^1.1.1",
    "husky": "^0.14.3",
    "imports-loader": "^0.8.0",
    "istanbul": "^0.4.4",
    "lodash": "^4.14.2",
    "minimist": "^1.2.0",
    "mocha": "^2.5.3",
    "mocha-junit-reporter": "^1.15.0",
    "moment-timezone": "^0.5.14",
