    "babel-preset-react": "^6.24.1",
    "bootstrap": "4.0.0",
    "bootstrap-select-v4": "^1.12.4",
    "bootstrap-slider": "^11.0.2",
    "classnames": "^2.2.6",
    "cleave.js": "^1.0.2",
    "flag-icon-css": "^2.9.0",
    "font-awesome": "^4.6.3",
    "imagesloaded": "^4.1.4",
    "install": "^0.11.0",
    "jquery": "^3.1.1",
    "jquery-countdown": "^2.2.0",
    "lazysizes": "^5.2.2",
    "line-reader": "^0.4.0",
    "packery": "^2.1.1",
    "popper.js": "^1.14.3",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "sha512": "0.0.1",
    "sticky-sidebar": "^3.3.1",
    "styled-components": "^4.1.3",
    "swiper": "^6.0.1"
  },
  "browserslist": [
    "last 2 versions",
    "ie >= 10"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
        "pre-push": "npm run test"
    }
  }
}
